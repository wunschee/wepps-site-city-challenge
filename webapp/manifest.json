{
	"_version": "1.8.0",
	"sap.app": {
		"id": "city.challenge",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "city.challenge.view.Main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mock.json"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "city.challenge.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "city.challenge.view",
				"targetAggregation": "pages",
				"clearTarget": false
			},
			"routes": [{
				"pattern": "",
				"name": "App",
				"view": "App",
				"targetControl": "app",
				"targetAggregation": "pages"
			}, {
				"pattern": "Challenge",
				"name": "Challenge",
				"view": "Challenge",
				"targetControl": "app",
				"targetAggregation": "pages"
			}, {
				"pattern": "Root",
				"name": "Root",
				"view": "Root",
				"targetControl": "app",
				"targetAggregation": "pages",
				"subroutes": [{
					"pattern": "Root/",
					"name": "Master",
					"view": "Master",
					"targetControl": "idRoot",
					"targetAggregation": "masterPages",
					"subroutes": [{
						"pattern": "Root/",
						"name": "Detail",
						"view": "Detail",
						"targetAggregation": "detailPages"
					}]
				}]
			}]
		}
	}
}